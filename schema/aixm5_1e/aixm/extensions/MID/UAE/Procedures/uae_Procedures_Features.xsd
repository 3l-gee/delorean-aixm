<!-- Component: UAE: Procedures: Features -->
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:aixm="http://www.aixm.aero/schema/5.1" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:uae_proc="http://www.aixm.aero/schema/5.1/extensions/UAE/Procedures" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.aixm.aero/schema/5.1/extensions/UAE/Procedures" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.1.a">
	<import namespace="http://www.opengis.net/gml/3.2" schemaLocation="../../../../ISO_19136_Schemas/gml.xsd"/>
	<import namespace="http://www.w3.org/1999/xlink" schemaLocation="../../../../xlink/xlink.xsd"/>
	<import namespace="http://www.aixm.aero/schema/5.1" schemaLocation="../../../../AIXM_Features.xsd"/>
	<element name="FinalProfileExtension" type="uae_proc:FinalProfileExtensionType" substitutionGroup="aixm:AbstractFinalProfileExtension"/>
	<complexType name="FinalProfileExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="uae_proc:FinalProfileExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="FinalProfileExtensionPropertyGroup">
		<sequence>
			<element name="rateOfDescent" type="uae_proc:RateOfDescentPropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<element name="RateOfDescent" type="uae_proc:RateOfDescentType"/>
	<complexType name="RateOfDescentType">
		<complexContent>
			<extension base="aixm:AbstractAIXMObjectType">
				<sequence>
					<group ref="uae_proc:RateOfDescentPropertyGroup"/>
					<element name="extension" minOccurs="0" maxOccurs="unbounded">
						<complexType>
							<choice>
								<element ref="uae_proc:AbstractRateOfDescentExtension"/>
							</choice>
							<attributeGroup ref="gml:OwnershipAttributeGroup"/>
						</complexType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="RateOfDescentPropertyGroup">
		<sequence>
			<element name="speed" type="aixm:ValSpeedType" nillable="true" minOccurs="0"/>
			<element name="speedReference" type="aixm:CodeSpeedReferenceType" nillable="true" minOccurs="0"/>
			<element name="rateOfDescent" type="aixm:ValSpeedType" nillable="true" minOccurs="0"/>
			<element name="descentGradient" type="aixm:ValSlopeType" nillable="true" minOccurs="0"/>
			<element name="annotation" type="aixm:NotePropertyType" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</group>
	<complexType name="RateOfDescentPropertyType">
		<complexContent>
			<extension base="aixm:AbstractAIXMPropertyType">
				<sequence>
					<element ref="uae_proc:RateOfDescent"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<element name="AbstractRateOfDescentExtension" type="aixm:AbstractExtensionType" abstract="true" substitutionGroup="aixm:AbstractExtension"/>
</schema>
