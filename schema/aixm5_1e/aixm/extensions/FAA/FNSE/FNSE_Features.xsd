<!-- Component: fnse: Features -->
<schema xmlns:fnse="http://www.aixm.aero/schema/5.1/extensions/FAA/FNSE" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:aixm="http://www.aixm.aero/schema/5.1" xmlns:event="http://www.aixm.aero/schema/5.1/event" targetNamespace="http://www.aixm.aero/schema/5.1/extensions/FAA/FNSE" elementFormDefault="qualified" attributeFormDefault="unqualified" version="5.1-a">
	<import namespace="http://www.opengis.net/gml/3.2" schemaLocation="../../../ISO_19136_Schemas/gml.xsd"/>
	<import namespace="http://www.w3.org/1999/xlink" schemaLocation="../../../xlink/xlink.xsd"/>
	<import namespace="http://www.aixm.aero/schema/5.1" schemaLocation="../../../AIXM_Features.xsd"/>
	<import namespace="http://www.aixm.aero/schema/5.1/event" schemaLocation="../../../event/Event_Features.xsd"/>
	<include schemaLocation="./FNSE_DataTypes.xsd"/>
	<annotation>
		<appinfo>
			<gml:gmlProfileSchema>http://www.aixm.aero/schema/5.0/profile/gml4aixm.xsd</gml:gmlProfileSchema>
		</appinfo>
	</annotation>
	<annotation>
		<documentation>Definition of classes that are specific to the FAA extension of the Event Schema.</documentation>
	</annotation>
	<element name="EventExtension" type="fnse:EventExtensionType" substitutionGroup="event:AbstractEventExtension"/>
	<complexType name="EventExtensionType">
		<complexContent>
			<extension base="aixm:AbstractExtensionType">
				<sequence>
					<group ref="fnse:EventExtensionPropertyGroup"/>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<group name="EventExtensionPropertyGroup">
		<sequence>
			<element name="classification" type="fnse:CodeNOTAMClassificationBaseType" nillable="true" minOccurs="0">
				<annotation>
					<appinfo>
						<gml:description>The most recent date and time when the information encoded in the Event was updated.</gml:description>
					</appinfo>
				</annotation>
			</element>
		</sequence>
	</group>
</schema>
