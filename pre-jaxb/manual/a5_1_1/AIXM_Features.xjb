				</hj:one-to-one>
			</jaxb:bindings>
			<jaxb:bindings node="//xs:group[@name='ElevatedSurfacePropertyGroup']//xs:element[@name='elevation']">
				<hj:embedded/>
			</jaxb:bindings>
			<jaxb:bindings node="//xs:group[@name='ElevatedSurfacePropertyGroup']//xs:element[@name='geoidUndulation']">
				<hj:embedded/>
			</jaxb:bindings>
			<jaxb:bindings node="//xs:group[@name='ElevatedSurfacePropertyGroup']//xs:element[@name='verticalDatum']">
				<hj:embedded/>
			</jaxb:bindings>
        
        </jaxb:bindings>
    </jaxb:bindings>
</jaxb:bindings>